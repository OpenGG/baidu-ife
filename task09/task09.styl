*
  margin: 0
  padding: 0

header
  background-color: #2289bf
  height: 57px
  width:100%
  min-width: 1280px;
  position: relative
header>img
  position: absolute
  left:50px
  top:10px
header>form
  position: absolute
  right:100px
  top:15px
  float:left
header:after
    content: ''
    clear:both
    display: block
header form input
  width: 313px
  height: 28px
  line-height: 28px
  border:none
  border-radius: 25px
  display: inline-block
  outline: none
  background-image: url(images/search_icon.png)
  background-repeat: no-repeat
  background-position: 15px 5px
  text-indent: 35px
header>input
  color: #FFFFFF
  font: 12px/2.25 'Adobe Heiti Std'
  background-color:  #2289bf
  border:none
  outline: none;
  padding:0 35px
  background-image: url(images/login.png)
  background-repeat: no-repeat
  background-position: 15px 5px
  position: absolute
  right:2px
  top:15px


div.container
  width:100%;
  min-width: 1280px;
  margin:0 auto
  display: flex;
  flex-direction: row;

aside
  background-color:#2a2e3d
  width: 260px
  height: 1213px
aside>figure
  display: flex
  flex-direction: row
  justify-content: center
  align-items: center
  padding:15px
aside>figure>img
  width:57px;
  height:57px;
  border-radius: 50%;
  border:1px solid #d4d4d6;
  padding:3px;

aside>figure>figcaption
  color: #F6F5F5
  font: 16px 'SimHei'
  padding:0 23px
aside>ul
  display: flex
  justify-content: center
  align-content: center
aside>ul>li
  list-style-type: none
  display: flex
  flex-direction: column
  text-align: center
  color: #6E8095
  font: 12px 'SimHei'
  padding:0 35px
aside>ul>li span:first-child
   font-size:14px
aside>ul>li:first-child
  border-right: 1px solid #6E8095

aside>nav
  padding:10px 40px
aside>nav li
    list-style-type: none
aside>nav>ul span.dir_name
  color: #6E8095
  font:  12px/4 'SimHei'
  padding-left:12px

aside>nav>ul span.icon
  background-repeat: no-repeat
  display: inline-block
  width: 12px
  height: 12px

aside>nav>ul li.report>.icon
  background-image: url(images/report.png)
aside>nav>ul li.file1>.icon
  background-image: url(images/file1.png)
  padding-right:3px
aside>nav>ul li.file1 .icon:nth-of-type(2)
  background-image: url(images/file.png)
aside>nav>ul li.file2>.icon
  background-image: url(images/file2.png)
aside>nav>ul li.file2>.icon:nth-of-type(1)
  background-image: url(images/11.png)
  padding-right:3px
  background-position: 0px 4px
aside>nav>ul li.doc1>.icon
  background-image: url(images/doc1.png)
aside>nav>ul li.doc2>.icon
  background-image: url(images/doc.png)
aside>nav>ul li.file3>.icon
  background-image: url(images/file1.png)
aside>nav>ul li.file3 .icon:nth-of-type(2)
  background-image: url(images/file.png)
aside>nav>ul li.collection>.icon
  background-image: url(images/star.png)
aside>nav>ul li.myshare>.icon
  background-image: url(images/myshare.png)
aside>nav>ul li.gallery>.icon
  background-image: url(images/star.png)
aside>nav>ul li.growth_record>.icon
  background-image: url(images/star.png)
aside>nav>ul li.account>.icon
  background-image: url(images/account.png)
aside>nav>ul .second
  padding-left:30px
aside>nav>ul .third
  padding-left: 55px
aside>nav>ul .second>.dir_name,
aside>nav>ul .third>.dir_name
  font:  12px/2.5 'SimHei'
  padding-left:5px
aside>nav>ul .report>.dir_name,
aside>nav>ul .file2>.dir_name,
aside>nav>ul .doc1>.dir_name
  color:#fff



