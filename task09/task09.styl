.search_form_container
  width:890px
  height: 160px
  margin:20px auto

  .search_form
    height:85px
    background-image: url(images/change.png)
    background-repeat: no-repeat
    background-position: 333px 25px

  fieldset
    border: none
    position: relative
    background-color: #fff
    border-top:1px solid #ddd
    border-left:1px solid #ddd
    &:after
      content: ''
      height:85px
      display: block
    legend
      display: none
    a
      font-size: 12px
      font-family: "SimHei"
      color: rgb(255, 255, 255)
      line-height: 2.167
      text-decoration: none
      position:absolute
      left:346px
      top:30px
    label
      font-size: 12px
      font-family: "SimHei"
      color: rgb(170, 170, 170)
      line-height: 2.167
    select
      font-size: 12px
      font-family: "SimHei"
      color: rgb(50, 51, 51)
      line-height: 2.167
      outline:none
      height: 25px
      text-indent: 5px
    section
      position: absolute
    .order
      left:30px
      top:15px
      select
        width:70px
        margin-left:16px
    .buyer
      left:180px
      top:15px
      select
        width:110px
        margin-left:16px
    .seller
      left:180px
      top:45px
      select
        width:110px
        margin-left:16px
    .date
      left:400px
      top:15px
      input
        width:120px
        margin-left:16px
        text-indent: 5px
        font-size: 12px
        font-family: "SimHei"
        color: rgb(50, 51, 51)
        outline:none
        height: 22px
    .num_of_pp
      left:400px
      top:45px
      select
        width:70px
        margin-left:16px
    .num_of_deal
      left:530px
      top:45px
      select
        width:120px
        margin-left:16px
    .time_total
      left:580px
      top:15px
      select
        width:70px
        margin-left:16px
    .search_btn
      left:730px
      top:30px
      input
        width:50px
        height: 25px
        background-color: #388ac1
        font-size: 12px
        font-family: "SimHei"
        color: rgb(255, 255, 255)
        line-height: 2.167
        border:none



  .search_history
    height: 65px
    margin-top:10px

    display:flex
    flex-direction:row
    justify-content: space-between
    div
      background-color: #fff
      border-top:1px solid #ddd
      border-left:1px solid #ddd
      height:65px
    .toleft,
    .toright
      width:30px
      background-repeat: no-repeat
      background-position:center
    .toleft
      background-image: url(images/arrow_left.png)
    .toright
      background-image: url(images/arrow_right.png)
    .history
      width:110px
      display: flex
      flex-direction: column
      justify-content: center
      align-items: center
      p:nth-of-type(1)
        font-size: 12px
        font-family: "Microsoft YaHei"
        color: rgb(50, 51, 51)
        line-height: 1.2
        margin-bottom: 10px
      p
        span:nth-of-type(1)
          font-size: 12px
          font-family: "Microsoft YaHei"
          color:#aeaeae
          font-weight: bolder
        span:nth-of-type(2)
          font-size: 21px
          font-family: "Microsoft YaHei"
          color: rgb(236, 101, 0)
          line-height: 0.686
          -moz-transform: scale(0.949, 1)
          -webkit-transform: scale(0.949, 1)
          -ms-transform: scale(0.949, 1)
    .history:hover
      border-top:3px solid #4389c9
      box-sizing: border-box
    .history_query
      width:130px
      display: flex
      align-items: center
      justify-content: flex-end
      background-image: url(images/search_history.jpg)
      background-repeat: no-repeat
      background-position: 20px 15px

      p
        font-size: 12px
        font-family: "Microsoft YaHei"
        color: rgb(83, 83, 83)
        line-height: 1.381
        margin:20px



.stat
  width:890px
  margin:20px auto
  height: 95px
  margin-bottom:10px
  p
    font-size: 12px
    font-family: "Microsoft YaHei"
    color: rgb(50, 51, 51)
