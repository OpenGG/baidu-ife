placeholder()
  &::-webkit-input-placeholder
    {block}
  &::-moz-placeholder
    {block}
  &:-ms-input-placeholder
    {block}

*
  margin: 0
  padding: 0

input
textarea
  +placeholder()
    color inherit
    font-size: inherit
    font-family: inherit
    line-height: inherit
input
select
button
  font-size: inherit;
  font-family: inherit;
  line-height: inherit
li
  list-style-type: none
html
  height: 100%

