price_width = 94px

//V project search
.proj_submit
  width: 350px
.search_proj
  padding 15px 20px 20px
  height: 270px;
  border:none;
  &:after
    conten:""
    display:block
    clear:both
  input[type="text"]
    width 100%
  label
    display: block;
    font-family: "Microsoft YaHei";
    color: rgb(195, 194, 194);
    line-height: 1.381;
    text-indent:5px;
  section
    .user_input,
    .user_description
      border:none;
      height:30px;
      outline:none;
      text-indent:5px;
    .user_description
      height:120px;
      padding-top:8px;
      resize:none;
  .proj_title
    padding-bottom: 0;
    border-bottom:1px solid #ddd;
  .proj_price
    float:left;
    width: price_width;
    border-right:1px solid #ddd;
    padding-top:2px
  .proj_desc
    border-top:1px solid #ddd;
    border-bottom:1px solid #ddd;
    padding-top:2px
  .proj_manager
    padding-top:2px
    margin-left price_width + 1px
  .proj_agreement
    display: flex
    align-items: center
    height 34px
    .checkbox
    .checkbox-replace
      width 13px
      height 13px
      border 0
    .checkbox
      opacity 0
    .checkbox-replace
      background: #d5d6d5 no-repeat 50% 50%
      margin-left -12px
    .checkbox:checked+.checkbox-replace
      background-image: url(images/checked.png)
    label
      padding-left:8px;
      display: inline;
    button
      margin-left auto
      background-color:#388ac1
      color: #fff;
      line-height: 2.167;
      border:none;
      padding:0 12px;
